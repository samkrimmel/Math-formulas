#solvinglinearsystem.py - finds the point of intersection between two lines

print('Ax+By=C')
print('Cx+Dy=F')

A = float(input('A = '))
B = float(input('B = '))
C = float(input('C = '))
D = float(input('D = '))
E = float(input('E = '))
F = float(input('F = '))

x = ((C-(B*(((A*F)-(D*C))/((-D*B)+(A*E)))))/A)
y = (((A*F)-(D*C))/((-D*B)+(A*E)))

print('(x,y) =',(x,y))
